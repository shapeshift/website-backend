{
  "kind": "collectionType",
  "collectionName": "supported_protocols",
  "info": {
    "singularName": "supported-protocol",
    "pluralName": "supported-protocols",
    "displayName": "Supported Protocol",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "featuredImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "collabDescription": {
      "type": "text",
      "required": true
    },
    "isFeatured": {
      "type": "boolean"
    },
    "features": {
      "type": "customField",
      "default": "[]",
      "options": [
        "OG DeFi Platforms",
        "Shift to DeFi on the go",
        "Get Paid to Trade",
        "Powered by Portals"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "cards": {
      "type": "component",
      "repeatable": true,
      "max": 3,
      "component": "blocks.protocol-card"
    }
  }
}
